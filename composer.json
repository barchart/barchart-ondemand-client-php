{
    "name": "barchart/ondemand-client",
    "description": "A client for Barchart OnDemand market data.",
    "keywords": [
        "barchart",
        "ondemand-client",
        "market-data",
        "futures",
        "forex",
        "stocks",
        "crypto",
        "cryptocurrencies"
    ],
    "homepage": "https://www.barchart.com/ondemand",
    "support": {
        "issues": "https://github.com/barchart/barchart-ondemand-client-php/issues",
        "source": "https://github.com/barchart/barchart-ondemand-client-php"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Sisk",
            "email": "tom.sisk@barchart.com",
            "homepage": "https://www.barchart.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "Barchart\\OnDemand\\": "src"
        }
    },
    "suggest": {
        "ext-curl": "Required to use CURL requests."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
